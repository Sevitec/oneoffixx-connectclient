<UserControl x:Class="OneOffixx.ConnectClient.WinApp.ViewContent.License"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OneOffixx.ConnectClient.WinApp.ViewContent"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="-80,0,-80,0">
        <TextBlock VerticalAlignment="Top" FontSize="14" FontWeight="Bold" Text="Licenses" />
        <ScrollViewer Height="300">
            <TextBlock TextWrapping="Wrap" VerticalAlignment="Center" Name="Content" />
        </ScrollViewer>
    </Grid>
</UserControl>
